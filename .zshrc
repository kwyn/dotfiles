# Git aliases
alias g='git'
alias gco='git checkout'
alias gdf='git diff'
alias glg='git log -p'
alias ggraph ='git log --graph --decorate --pretty=oneline --abrev-commit --all'
alias gc='git commit'
alias greset='git reset --head HARD'
alias gcane='git commit --amend --no-edit'
alias gca='git commit --amend'
alias ga='git add'
alias gb='git branch'

# Reload Alias
alias reloead='source ~/.zshrc'

